<mvc:View controllerName="sap.com.listbindingfromodata.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
            <List items="{/Products}">
                <items>
                    <StandardListItem title="{ProductName}"  description="{ProductID}"/>
                </items>
            </List>
        </content>
    </Page>
</mvc:View>
