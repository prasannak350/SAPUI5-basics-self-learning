{
  "_version": "1.65.0",
  "sap.app": {
    "id": "sap.com.routingnavigation",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.15.1",
      "toolsId": "220b5e2f-9d6c-4129-8190-d8b1ef961fd1"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.128.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "sap.com.routingnavigation.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "sap.com.routingnavigation.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteRootView",
          "pattern": "RouteRootView",
          "target": [
            "TargetRootView"
          ]
        },
        {
          "name": "FirstPage",
          "pattern": "",
          "target": [
            "TargetFirstPage"
          ]
        },
        {
          "name": "SecondPage",
          "pattern": "SecondPage/{name}",
          "target": [
            "TargetSecondPage"
          ]
        }

      ],
      "targets": {
        "TargetRootView": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "RootView",
          "viewName": "RootView"
        },
        "TargetFirstPage": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "FirstPage",
          "viewName": "FirstPage"
        },
        "TargetSecondPage": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "SecondPage",
          "viewName": "SecondPage"
        }
      }
    },
    "rootView": {
      "viewName": "sap.com.routingnavigation.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
